@model IEnumerable<SeatsProject.Models.Prenotazioni>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Utente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodicePostazione)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Utente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodicePostazione)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPrenotazine">Edit</a> |
